select without table
explain
SELECT 1
------
Projection: 1
  NoRelation

select *
explain
SELECT * from users
------
Projection: users.id, users.username, users.age, users.state, users.wallet
  Scan: users; projection=[]

select columns
explain
select username, age from users
------
Projection: users.username, users.age
  Scan: users; projection=[]

